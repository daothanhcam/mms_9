<%= form_for [:admin, @project] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
    <table class="table">
      <tr>
        <th><%= t "project.in_project_users" %><th>
      </tr>
      <%= f.collection_check_boxes :user_ids, @in_project_users,
                                   :id, :username do |b| %>
        <tr>
          <%= b.label do %>
            <th><%= b.text %></th>
            <th class="selection_checkbox"><%= b.check_box %></th>
          <% end %>
        </tr>
      <% end %>
    </table>
  <%= f.submit t("project.deletemember"), class: "btn btn-primary" %>
<% end %>
